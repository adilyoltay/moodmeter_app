var _trend = require("../../utils/trend");
describe('getTrendFromP50', function () {
  it('returns stable for empty or single value', function () {
    expect((0, _trend.getTrendFromP50)([])).toBe('stable');
    expect((0, _trend.getTrendFromP50)([50])).toBe('stable');
  });
  it('ignores NaN and non-finite values', function () {
    expect((0, _trend.getTrendFromP50)([NaN, undefined, null, 40, 60])).toBe('up');
  });
  it('applies threshold', function () {
    expect((0, _trend.getTrendFromP50)([50, 55], 10)).toBe('stable');
    expect((0, _trend.getTrendFromP50)([50, 61], 10)).toBe('up');
    expect((0, _trend.getTrendFromP50)([60, 49], 10)).toBe('down');
  });
});
