exports.isUUID = isUUID;
function isUUID(value) {
  if (!value || typeof value !== 'string') return false;
  var uuidPattern = /^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$/i;
  return uuidPattern.test(value);
}
