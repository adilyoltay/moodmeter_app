var _chartSelection = require("../../components/mood/helpers/chartSelection");
describe('chart selection helpers', function () {
  test('getXLabelVisibility respects day spacing', function () {
    expect((0, _chartSelection.getXLabelVisibility)(0, 24, 'day', 200)).toBe(true);
    expect((0, _chartSelection.getXLabelVisibility)(1, 24, 'day', 200)).toBe(false);
    expect((0, _chartSelection.getXLabelVisibility)(4, 24, 'day', 200)).toBe(true);
  });
  test('getXLabelVisibility shows edges', function () {
    expect((0, _chartSelection.getXLabelVisibility)(0, 10, 'month', 300)).toBe(true);
    expect((0, _chartSelection.getXLabelVisibility)(9, 10, 'month', 300)).toBe(true);
  });
  test('formatXLabel day uses HH', function () {
    var item = {
      date: '2025-09-07#03'
    };
    expect((0, _chartSelection.formatXLabel)(item, 'day', 'tr-TR')).toBe('03');
  });
  test('formatXLabel week uses weekday short', function () {
    var item = {
      date: '2025-09-08'
    };
    var res = (0, _chartSelection.formatXLabel)(item, 'week', 'tr-TR');
    expect(['Pz', 'Pt', 'Sa', 'Ã‡a', 'Pe', 'Cu', 'Ct']).toContain(res);
  });
  test('formatXLabel 6months uses month short', function () {
    var item = {
      date: '2025-01-01'
    };
    var res = (0, _chartSelection.formatXLabel)(item, '6months', 'tr-TR');
    expect(res.length).toBeGreaterThanOrEqual(2);
  });
});
