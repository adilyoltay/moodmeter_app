var _heuristicFallback = require("../../features/ai/services/heuristicFallback");
describe('heuristicFallback', function () {
  it('detects anxiety scenario', function () {
    var text = (0, _heuristicFallback.getHeuristicText)([{
      role: 'user',
      content: 'çok kaygılıyım',
      id: '1',
      timestamp: new Date()
    }], {});
    expect(typeof text).toBe('string');
  });
  it('respects preferredLanguage=en', function () {
    var lang = (0, _heuristicFallback.detectLanguage)({
      therapeuticProfile: {
        preferredLanguage: 'en'
      }
    });
    expect(lang).toBe('en');
  });
});
