exports.createScrollSteps = createScrollSteps;
exports.inertialInterpolator = inertialInterpolator;
exports.lerp = lerp;
exports.linearInterpolator = linearInterpolator;
var DEFAULT_STEPS_COUNT = 5;
function createScrollSteps(target, initialOffset, interpolator) {
  if (target.y != null) {
    return interpolator(target.y, initialOffset.y, DEFAULT_STEPS_COUNT).map(function (y) {
      return {
        y: y,
        x: initialOffset.x
      };
    });
  }
  if (target.x != null) {
    return interpolator(target.x, initialOffset.x, DEFAULT_STEPS_COUNT).map(function (x) {
      return {
        x: x,
        y: initialOffset.y
      };
    });
  }
  return [];
}
function linearInterpolator(end, start, steps) {
  if (end === start) {
    return [end, start];
  }
  var result = [];
  for (var i = 0; i < steps; i += 1) {
    result.push(lerp(start, end, i / (steps - 1)));
  }
  return result;
}
function inertialInterpolator(end, start, steps) {
  if (end === start) {
    return [end, start];
  }
  var result = [];
  var factor = 1;
  for (var i = 0; i < steps - 1; i += 1) {
    result.push(lerp(end, start, factor));
    factor /= 2;
  }
  result.push(end);
  return result;
}
function lerp(v0, v1, t) {
  return v0 + t * (v1 - v0);
}
