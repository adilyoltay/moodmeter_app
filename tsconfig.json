{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "paths": {
      "@/assets/*": [
        "./assets/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/constants/*": [
        "./constants/*"
      ],
      "@/hooks/*": [
        "./hooks/*"
      ],
      "@/store/*": [
        "./store/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ],
      "@/services/*": [
        "./services/*"
      ],
      "@/contexts/*": [
        "./contexts/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/localization/*": [
        "./localization/*"
      ],
      "@/features/*": [
        "./features/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "backup/**",
    "archive/**",
    "__tests__/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "supabase/functions/**",
    "features/ai-disabled-backup/**",
    "app/(dev)/**",
    "components/dev/**",
    "components/ai/**",
    "hooks/useAIErrorHandler.ts",
    "utils/debugInvalidEntries.ts"
  ]
}
